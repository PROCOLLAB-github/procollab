{
  "name": "procollab",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve procollab-dev",
    "build": "npm run build:sprite && ng build",
    "build:prod": "npm run build --configuration procollab",
    "build:pr": "npm run build --configuration procollab-dev",
    "watch": "ng build --watch --configuration procollab-dev",
    "build:sprite": "svg-sprite -s --dest=src/assets/icons 'src/assets/icons/svg/**/*.svg'",
    "test": "ng test",
    "test:ci": "ng test --browsers=Headless --no-watch",
    "lint:ts": "eslint src/**/*.ts",
    "format": "prettier --write src/",
    "lint:scss": "stylelint src/**/*.scss --fix",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d ."
  },
  "precommit": [
    "lint:scss",
    "lint:ts"
  ],
  "private": true,
  "engines": {
    "node": "~18.16"
  },
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/cdk": "^16.2.12",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/material": "^16.2.12",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "camelcase-keys": "^8.0.1",
    "class-transformer": "^0.5.1",
    "core-js": "^3.23.4",
    "dayjs": "^1.11.3",
    "fuse.js": "^6.6.2",
    "linkify-string": "^4.1.1",
    "linkifyjs": "^4.1.1",
    "nanoid": "^4.0.0",
    "ng-click-outside": "^9.0.1",
    "ngx-autosize": "^2.0.4",
    "ngx-mask": "^13.0.3",
    "rxjs": "~7.5.0",
    "snakecase-keys": "^5.4.2",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@babel/core": "^7.18.6",
    "@compodoc/compodoc": "^1.1.19",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-loader": "^8.2.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "intl": "^1.2.5",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "svg-sprite": "^2.0.2",
    "typescript": "~5.2"
  }
}