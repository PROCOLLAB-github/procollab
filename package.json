{
  "name": "procollab",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:mock": "./node_modules/.bin/json-server --delay 200 --no-corse -p 3333 --watch mock.json -r routes.json",
    "build": "npm run build:sprite && ng build",
    "watch": "ng build --watch --configuration development",
    "build:sprite": "svg2sprite ./src/assets/icons/svg ./src/assets/icons/sprite.svg --stripAttrs class",
    "test": "ng test",
    "test:ci": "ng test --browsers=Headless --no-watch",
    "lint:ts": "eslint src/**/*.ts",
    "format": "prettier --write src/",
    "lint:scss": "stylelint src/**/*.scss",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "precommit": [
    "lint:scss",
    "lint:ts",
    "test:ci"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@sentry/angular": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "camelcase-keys": "^8.0.1",
    "class-transformer": "^0.5.1",
    "core-js": "^3.23.4",
    "dayjs": "^1.11.3",
    "ng-click-outside": "^9.0.1",
    "ngx-autogrow": "^3.0.1",
    "ngx-mask": "^13.0.3",
    "rxjs": "~7.5.0",
    "snakecase-keys": "^5.4.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.0",
    "@babel/core": "^7.18.6",
    "@compodoc/compodoc": "^1.1.19",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/angular": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/testing-library": "0.0.13",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "babel-loader": "^8.2.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-storybook": "^0.6.1",
    "intl": "^1.2.5",
    "jasmine-core": "~4.0.0",
    "json-server": "^0.17.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "svg2sprite-cli": "^2.0.1",
    "typescript": "~4.6.2"
  }
}
