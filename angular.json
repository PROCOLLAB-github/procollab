            "outputPath": "dist/skills",
            "index": "projects/skills/src/index.html",
            "main": "projects/skills/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/skills/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/skills/src/favicon.ico",
