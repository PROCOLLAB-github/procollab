{
  "env": {
    "browser": true,
    "es2021": true,
    "jasmine": true
  },
  "extends": [
    "standard",
    "prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-useless-constructor": "off",
    "@typescript-eslint/no-empty-function": "off",
    "dot-notation": "off"
  }
}
